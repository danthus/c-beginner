#include "math.h"
#define _USE_MATH_DEFINES
#include <iostream>
using namespace std;

int successiveproduct(int n)
{
    int product=1;
    for(int i=1;i<=n;i++)
        product=product*i;
    return product;
}
        


int main()
{
    cout<<"enter a integer : ";
    int a;
    cin>>a;
    
    cout<<"the product of all integer before "<<a<<" = "<<successiveproduct(a)<<endl;
    return 0;
}
