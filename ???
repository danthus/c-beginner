#include <iostream>
#include <string>
using namespace std;

int main()
{
    string A;
    for(int i=0;i<26;i++)
    {
        A[i]=65+i;
        cout<<A[i];
    }
    cout<<endl;
    cout<<A;

    
    return 0;
}


#include <iostream>
#include <string>
using namespace std;

int main()
{
    string A;
    for(int i=0;i<26;i++)
    {
        char s=65+i;
        A=A+s;
    }
    cout<<A;

    
    return 0;
}
